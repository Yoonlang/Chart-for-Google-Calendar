{
  "manifest_version": 3,

  "name": "Chart for Google Calendar",
  "description": "Support statistics for Google Calendar by chart",
  "version": "1.0",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "host_permissions": ["<all_urls>"]
}
